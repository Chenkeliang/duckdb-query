{
  "debug": false,
  "cors_origins": [
    "http://localhost:3000",
    "http://localhost:5173"
  ],
  "max_file_size": 53687091200,
  "query_timeout": 300,
  "download_timeout": 600,
  "max_query_rows": 10000,
  "max_tables": 1000,
  "enable_caching": true,
  "cache_ttl": 3600,
  "timezone": "Asia/Shanghai",
  "enable_pivot_tables": true,
  "pivot_table_extension": "pivot_table",
  "duckdb_memory_limit": "8GB",
  "duckdb_threads": 8,
  "duckdb_temp_directory": null,
  "duckdb_home_directory": null,
  "duckdb_extension_directory": null,
  "duckdb_data_dir": null,
  "duckdb_database_path": null,
  "duckdb_enable_profiling": "query_tree",
  "duckdb_profiling_output": null,
  "duckdb_prefer_range_joins": false,
  "duckdb_enable_object_cache": true,
  "duckdb_preserve_insertion_order": false,
  "duckdb_enable_progress_bar": false,
  "duckdb_extensions": [
    "excel",
    "json",
    "parquet",
    "httpfs"
  ],
  "duckdb_remote_settings": {},
  "server_data_mounts": [
    {
      "label": "Local Server Data",
      "path": "/app/server_mounts"
    },
    {
      "label": "macOS Downloads",
      "path": "/app/host_downloads"
    },
    {
      "label": "macOS Documents",
      "path": "/app/host_documents"
    }
  ],
  "duckdb_debug_logging": false,
  "duckdb_auto_explain_threshold_ms": 0,
  "exports_dir": null,
  "temp_files_dir": null,
  "pool_min_connections": 2,
  "pool_max_connections": 10,
  "pool_connection_timeout": 30,
  "pool_idle_timeout": 300,
  "pool_max_retries": 3,
  "db_connect_timeout": 3,
  "db_read_timeout": 30,
  "db_write_timeout": 30,
  "db_ping_timeout": 2,
  "query_proxy_timeout": 300,
  "url_reader_timeout": 30,
  "url_reader_head_timeout": 10,
  "sqlite_timeout": 10,
  "pool_wait_timeout": 1.0
}