{
    "debug": false,
    "cors_origins": [
        "http://localhost:3000",
        "http://localhost:5173"
    ],
    "max_file_size": 53687091200,
    "query_timeout": 300,
    "download_timeout": 600,
    "max_query_rows": 10000,
    "max_tables": 200,
    "enable_caching": true,
    "cache_ttl": 3600,
    "timezone": "Asia/Shanghai",
    "duckdb_memory_limit": "8GB",
    "duckdb_threads": 8,
    "duckdb_temp_directory": "./data/duckdb/temp",
    "duckdb_home_directory": "./data/duckdb/home",
    "duckdb_extension_directory": "./data/duckdb/extensions",
    "duckdb_enable_profiling": true,
    "duckdb_profiling_output": "./data/duckdb/profile.json",
    "duckdb_force_index_join": false,
    "duckdb_enable_object_cache": true,
    "duckdb_preserve_insertion_order": false,
    "duckdb_enable_progress_bar": false,
    "duckdb_extensions": [
        "excel",
        "json",
        "parquet",
        "httpfs",
        "fts"
    ]
}